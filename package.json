{
  "name": "@fob/lib-ui",
  "type": "module",
  "version": "0.1.0",
  "description": "Shared UI component library for Fobrix applications",
  "main": "src/index.js",
  "module": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./package.json": "./package.json",
    "./primitives/*": "./src/primitives/*.jsx",
    "./primitives/data-table/*": "./src/primitives/data-table/*.jsx",
    "./components/*": "./src/components/*.jsx",
    "./components/magicui/*": "./src/components/magicui/*.jsx",
    "./components/CronScheduler": "./src/components/CronScheduler/index.js",
    "./components/CronScheduler/*": "./src/components/CronScheduler/*.jsx",
    "./lib/utils": "./src/lib/utils.js",
    "./utils/cron": "./src/utils/cron/index.js",
    "./hooks/*": "./src/hooks/*.js",
    "./styles/globals.css": "./src/styles/globals.css"
  },
  "scripts": {
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "npx serve storybook-static -l 6006",
    "storybook:test": "test-storybook --verbose --junit",
    "storybook:test:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run storybook:build && npm run storybook:serve\" \"wait-on tcp:6006 && npm run storybook:test\"",
    "test:components": "npm run storybook:test:ci"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@bprogress/next": "^3.0.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cron-parser": "^5.0.0",
    "cronstrue": "^2.50.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.0.0",
    "lucide-react": "^0.523.0",
    "next-themes": "^0.4.6",
    "prop-types": "^15.8.1",
    "react-day-picker": "^9.9.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.6",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.0",
    "@storybook/addon-docs": "^9.0.0",
    "@storybook/nextjs": "^9.0.0",
    "@storybook/test-runner": "^0.23.0",
    "@tailwindcss/postcss": "^4.1.11",
    "concurrently": "^9.2.0",
    "next": "^15.4.1",
    "playwright": "^1.54.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^9.0.0",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "wait-on": "^8.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:finopsbricks/lib-ui.git"
  },
  "keywords": [
